<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dbmasteraapp.Views.ResultadoPesquisa">
    <ContentPage.Content>
        <ListView SeparatorColor="Black" x:Name="listViewChamado" ItemSelected="listViewChamado_ItemSelected" IsPullToRefreshEnabled="True" Refreshing="listViewChamado_Refreshing">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding Codigo}" Detail="{Binding Titulo}">
                    </TextCell>
                </DataTemplate>
            </ListView.ItemTemplate>

        </ListView>
    </ContentPage.Content>
</ContentPage>