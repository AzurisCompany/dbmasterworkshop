<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dbmasteraapp.Views.CriarChamado"
             Title="Novo Chamado">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Projeto" />
            <Picker x:Name="PickerProjeto" ItemDisplayBinding="Descricao" />
            <Label Text="Tipo" />
            <Picker x:Name="PickerTipo" ItemDisplayBinding="Descricao" />
            <Label Text="Maquina" />
            <Picker x:Name="PickerMaquina" ItemDisplayBinding="Descricao" />
        </StackLayout>


    </ContentPage.Content>
</ContentPage>